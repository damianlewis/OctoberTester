<?php

use DamianLewis\OctoberTester\OctoberUiTestCase;

abstract class UiTestCase extends OctoberUiTestCase
{
    use CreatesApplication;

    /**
     * Get a callback that returns the default 'administrator' user credentials to authenticate.
     *
     * @return array
     */
    protected function getUserCredentials()
    {
        return [
            'login'    => 'admin',
            'password' => 'admin'
        ];
    }
}
